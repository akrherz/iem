<!-- ================================ -->
<!--           MANUFACTURER           -->
<!-- =====================T=========== -->
<!-- This section is used to identify the manufacturer -->

<sfl:manufacturer xlink:href="http://sawi.gst.com/nmpa/rp/rp_TexasElectronics.xml" />


<!-- ================================ -->
<!--             MODEL                -->
<!-- ================================ -->
<!-- This section is used to describe the manufacturer-designated model number -->

<sfl:model>TE525USW</sfl:model>



<!-- ================================ -->
<!--      OPERATIONAL COMPONENTS      -->
<!-- ================================ -->
<!-- These properties relate the operational aspects of the sensor -->

<sfl:operationalComponent>
  <swe:QuantityRange definition="http://sawi.gst.com/nmpa/docs/terms.html#operatingTemperature">
    <swe:uom code="K"/>                                                                   
    <swe:value>273.15 323.15</swe:value>     <!-- These SI units correspond to 0C and +50C -->
  </swe:QuantityRange>
</sfl:operationalComponent>

<sfl:operationalComponent>
  <swe:QuantityRange definition="http://sawi.gst.com/nmpa/docs/terms.html#storageTemperature">
    <swe:uom code="K"/>                                                                   
    <swe:value>233.15 343.15</swe:value>     <!-- These SI units correspond to -40C and +70C -->
  </swe:QuantityRange>
</sfl:operationalComponent>

<sfl:operationalComponent>
  <sfl:ComplexConditionalValue>
    <sfl:condition>
      <swe:QuantityRange definition="http://sawi.gst.com/nmpa/docs/terms.html#operatingCurrent">
        <swe:uom code="A"/>                                                                   
        <swe:value>0 2</swe:value>
      </swe:QuantityRange>
    </sfl:condition>
    <sfl:value>
      <swe:QuantityRange definition="http://sawi.gst.com/nmpa/docs/terms.html#operatingVoltage">
        <swe:uom code="VDC"/>                                                                   
        <swe:value>0 30</swe:value>
      </swe:QuantityRange>
    </sfl:value>      
  </sfl:ComplexConditionalValue>
</sfl:operationalComponent>


<!-- ================================ -->
<!--       PHYSICAL COMPONENTS        -->
<!-- ================================ -->
<!-- These properties relate the physical aspects of the sensor -->

<sfl:physicalComponent>
  <swe:Quantity definition="http://sawi.gst.com/nmpa/docs/terms.html#netWeight">
    <swe:uom code="kg"/>
    <swe:value>1.2</swe:value>
  </swe:Quantity>
</sfl:physicalComponent>

<sfl:physicalComponent>
  <swe:Quantity definition="http://sawi.gst.com/nmpa/docs/terms.html#height">
    <swe:uom code="m"/>
    <swe:value>0.28</swe:value>
  </swe:Quantity>
</sfl:physicalComponent>

<sfl:physicalComponent>
  <swe:Quantity definition="http://sawi.gst.com/nmpa/docs/terms.html#collectorDiameter">
    <swe:uom code="m"/>
    <swe:value>0.203</swe:value>
  </swe:Quantity>
</sfl:physicalComponent>

<sfl:physicalComponent>
  <swe:Quantity definition="http://sawi.gst.com/nmpa/docs/terms.html#funnelDepth">
    <swe:uom code="m"/>
    <swe:value>0.163</swe:value>
  </swe:Quantity>
</sfl:physicalComponent>

<sfl:physicalComponent>
  <swe:Quantity definition="http://sawi.gst.com/nmpa/docs/terms.html#splashOutProtection">
    <swe:uom code="m"/>
    <swe:value>0.050</swe:value>
  </swe:Quantity>
</sfl:physicalComponent>

<!-- ================================ -->
<!--       Provides: Rainfall         -->
<!-- ================================ -->

<sfl:provides>
 <sfl:SensingProcedure xlink:href="http://sawi.gst.com/nmpa/sensor/procedure/sp_TE-TE525USW_rainfall.xml" />
</sfl:provides>


